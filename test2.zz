module Test2;

class Foo {
    i32 value;

    fn i32 get_value(Foo self) {
        return self.value;
    }

    fn void set_value(Foo self, i32 v) {
        self.value = v;
        return;
    }
}

fn static i32 main() {
    let obj Foo;
    obj.set_value(obj, 42);
    return obj.get_value(obj);
}
